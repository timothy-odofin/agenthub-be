esilience"],"summary":"Get Resilience Health","description":"Get overall resilience health status.\n\nChecks all circuit breakers and reports:\n- Total circuits\n- Circuits by state (closed, open, half_open)\n- Any circuits that are open (indicating problems)\n\nReturns:\n    Health status summary","operationId":"get_resilience_health_api_v1_resilience_health_get","responses":{"200":{"description":"Successful Response","content":{"application/json":{"schema":{"additionalProperties":true,"type":"object","title":"Response Get Resilience Health Api V1 Resilience Health Get"}}}}}}},"/api/v1/resilience/summary":{"get":{"tags":["resilience","resilience"],"summary":"Get Resilience Summary","description":"Get a summary of all circuit breakers with key metrics.\n\nReturns:\n    List of circuit breaker summaries","operationId":"get_resilience_summary_api_v1_resilience_summary_get","responses":{"200":{"description":"Successful Response","content":{"application/json":{"schema":{"items":{"additionalProperties":true,"type":"object"},"type":"array","title":"Response Get Resilience Summary Api V1 Resilience Summary Get"}}}}}}},"/":{"get":{"summary":"Root","operationId":"root__get","responses":{"200":{"description":"Successful Response","content":{"application/json":{"schema":{}}}}}}},"/health":{"get":{"summary":"Health Check","operationId":"health_check_health_get","responses":{"200":{"description":"Successful Response","content":{"application/json":{"schema":{}}}}}}}},"components":{"schemas":{"ChatRequest":{"properties":{"message":{"type":"string","title":"Message"},"session_id":{"anyOf":[{"type":"string"},{"type":"null"}],"title":"Session Id"}},"type":"object","required":["message"],"title":"ChatRequest"},"ChatResponse":{"properties":{"success":{"type":"boolean","title":"Success"},"message":{"type":"string","title":"Message"},"session_id":{"type":"string","title":"Session Id"},"user_id":{"type":"string","title":"User Id"},"timestamp":{"type":"string","title":"Timestamp"},"processing_time_ms":{"type":"number","title":"Processing Time Ms"},"tools_used":{"items":{"type":"string"},"type":"array","title":"Tools Used","default":[]},"errors":{"items":{"type":"string"},"type":"array","title":"Errors","default":[]},"metadata":{"additionalProperties":true,"type":"object","title":"Metadata","default":{}}},"type":"object","required":["success","message","session_id","user_id","timestamp","processing_time_ms"],"title":"ChatResponse"},"ConversationalSignupRequest":{"properties":{"message":{"type":"string","title":"Message","description":"User's message/input for current step","examples":["john@example.com"]},"session_id":{"anyOf":[{"type":"string"},{"type":"null"}],"title":"Session Id","description":"Session ID from server response (null only for START step)","examples":["abc-123"]},"current_step":{"anyOf":[{"$ref":"#/components/schemas/SignupStep"},{"type":"null"}],"description":"Current step (MUST match next_step from previous response, null only for START)","examples":["email"]}},"type":"object","required":["message"],"title":"ConversationalSignupRequest","description":"Request schema for conversational signup with Redis session management.\n\nClient sends 3 fields:\n1. message: User's input (required)\n2. session_id: Tracking ID (null for START, required for all other steps)\n3. current_step: Current step (null for START, required for all other steps)\n\nAll validated data is stored server-side in Redis.\nThis prevents password exposure and ensures data integrity.\n\nFlow:\n- First request: {\"message\": \"\", \"session_id\": null, \"current_step\": \"start\"}\n- Server returns: session_id in response (e.g., \"abc-123\")\n- Subsequent requests: {\"message\": \"user input\", \"session_id\": \"abc-123\", \"current_step\": \"email\"}","examples":[{"current_step":"start","description":"Step 1: START - Initialize signup (session_id is null)","message":""},{"current_step":"email","description":"Step 2: EMAIL - Provide email (session_id from server response)","message":"john@example.com","session_id":"abc-123-uuid-from-server"},{"current_step":"username","description":"Step 3: USERNAME - Provide username (same session_id)","message":"johndoe","session_id":"abc-123-uuid-from-server"},{"current_step":"password","description":"Step 4: PASSWORD - Provide password (same session_id)","message":"SecurePass123","session_id":"abc-123-uuid-from-server"},{"current_step":"firstname","description":"Step 5: FIRSTNAME - Provide first name (same session_id)","message":"John","session_id":"abc-123-uuid-from-server"},{"current_step":"lastname","description":"Step 6: LASTNAME - Provide last name (same session_id)","message":"Doe","session_id":"abc-123-uuid-from-server"}]},"ConversationalSignupResponse":{"properties":{"success":{"type":"boolean","title":"Success","description":"Whether the current step was successful"},"message":{"type":"string","title":"Message","description":"Bot's response message to user"},"next_step":{"$ref":"#/components/schemas/SignupStep","description":"Next step in the signup process"},"session_id":{"type":"string","title":"Session Id","description":"Session ID for tracking conversation"},"is_valid":{"type":"boolean","title":"Is Valid","description":"Whether current input is valid","default":true},"validation_error":{"anyOf":[{"type":"string"},{"type":"null"}],"title":"Validation Error","description":"Validation error message if any"},"user_id":{"anyOf":[{"type":"string"},{"type":"null"}],"title":"User Id","description":"Created user ID if signup complete"},"access_token":{"anyOf":[{"type":"string"},{"type":"null"}],"title":"Access Token","description":"JWT access token if signup complete"},"refresh_token":{"anyOf":[{"type":"string"},{"type":"null"}],"title":"Refresh Token","description":"JWT refresh token if signup complete"},"token_type":{"type":"string","title":"Token Type","description":"Token type","default":"bearer"},"progress_percentage":{"type":"integer","title":"Progress Percentage","description":"Signup completion percentage (0-100)"},"fields_remaining":{"type":"integer","title":"Fields Remaining","description":"Number of fields still needed"}},"type":"object","required":["success","message","next_step","session_id","progress_percentage","fields_remaining"],"title":"ConversationalSignupResponse","description":"Response schema for conversational signup."},"CreateSessionRequest":{"properties":{"title":{"anyOf":[{"type":"string"},{"type":"null"}],"title":"Title"}},"type":"object","title":"CreateSessionRequest"},"CreateSessionResponse":{"properties":{"success":{"type":"boolean","title":"Success"},"session_id":{"type":"string","title":"Session Id"},"title":{"type":"string","title":"Title"},"created_at":{"type":"string","title":"Created At"},"errors":{"items":{"type":"string"},"type":"array","title":"Errors","default":[]}},"type":"object","required":["success","session_id","title","created_at"],"title":"CreateSessionResponse"},"DataSourceType":{"type":"string","enum":["s3","confluence","file","sharepoint","database","url","web","jira"],"title":"DataSourceType","description":"Supported data sources types for ingestion."},"HTTPValidationError":{"properties":{"detail":{"items":{"$ref":"#/components/schemas/ValidationError"},"type":"array","title":"Detail"}},"type":"object","title":"HTTPValidationError"},"LoginRequest":{"properties":{"identifier":{"type":"string","title":"Identifier","description":"Email or username"},"password":{"type":"string","title":"Password","description":"User password"}},"type":"object","required":["identifier","password"],"title":"LoginRequest","description":"Request schema for user login."},"LoginResponse":{"properties":{"success":{"type":"boolean","title":"Success","description":"Whether login was successful"},"message":{"type":"string","title":"Message","description":"Response message"},"access_token":{"anyOf":[{"type":"string"},{"type":"null"}],"title":"Access Token","description":"JWT access token if successful"},"refresh_token":{"anyOf":[{"t